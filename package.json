{
  "name": "ltsv",
  "version": "0.6.1",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "description": "LTSV parser and formatter",
  "main": "./index.js",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/sasaplus1/ltsv.git"
  },
  "scripts": {
    "bench": "node ./bench/index.js",
    "bower": "bower install",
    "fix": "fixjsstyle --strict --nojsdoc -r ./lib -r ./test ./index.js",
    "lint": "gjslint --strict --nojsdoc -r ./lib -r ./test ./index.js",
    "mini": "uglifyjs (scripts) --comments -bcm -d module='void 0' -e : -r ltsv -o ./ltsv.min.js",
    "test": "NODE_ENV=test mocha",
    "testem": "testem ci"
  },
  "devDependencies": {
    "benchmark": "~1.0",
    "bower": "~1.1",
    "expect.js": "~0.2",
    "mocha": "~1.12",
    "testem": "~0.3",
    "uglify-js": "~2.3"
  },
  "engines": {
    "node": ">0.9"
  }
}
