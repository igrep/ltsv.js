{
  "name": "ltsv",
  "version": "0.7.2",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "description": "LTSV parser and formatter",
  "main": "./index.js",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/sasaplus1/ltsv.js.git"
  },
  "scripts": {
    "bower": "bower install",
    "fix": "fixjsstyle --strict --nojsdoc -r ./lib -r ./test ./index.js",
    "lint": "gjslint --strict --nojsdoc -r ./lib -r ./test ./index.js",
    "mini": "uglifyjs ./lib/ltsv.js --comments -cm -d 'module=void 0' -r ltsv -o ./ltsv.min.js",
    "test": "NODE_ENV=test mocha",
    "testem": "testem ci",
    "travis": "testem ci --launch Firefox,PhantomJS"
  },
  "devDependencies": {
    "bower": "~1.2",
    "expect.js": "~0.2",
    "mocha": "~1.14",
    "testem": "~0.5",
    "uglify-js": "~2.4"
  },
  "engines": {
    "node": ">0.9"
  }
}
