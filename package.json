{
  "name": "ltsv",
  "description": "LTSV parser, formatter, validator and TransformStream",
  "version": "1.1.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "browser": "./umd/ltsv.js",
  "dependencies": {
    "core-js": "^3.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "espower-typescript": "^9.0.2",
    "husky": "3.0.4",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "1.3.0",
    "karma-rollup-preprocessor": "7.0.2",
    "lint-staged": "9.2.5",
    "mocha": "6.2.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "1.6.1",
    "prettier": "1.18.2",
    "rollup": "1.20.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "cjs",
    "index.*",
    "src",
    "umd"
  ],
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.mjs",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/ltsv.js.git"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "BABEL_ENV=cjs babel ./src -d ./cjs -s --ignore ./src/browser_stream.mjs && BABEL_ENV=cjs babel ./index.mjs -o ./index.js -s",
    "build:docs": "jsdoc --configure ./.jsdocrc.js",
    "build:umd": "BABEL_ENV=umd rollup -c",
    "clean": "npx rimraf ./cjs ./umd ./index.js ./index.js.map",
    "fix": "eslint --ext .js,.mjs --fix .",
    "fixpack": "npx fixpack",
    "karma": "BABEL_ENV=karma karma start --no-browsers --file ./umd/ltsv.js",
    "lint": "eslint --ext .js,.mjs .",
    "mocha:cjs": "BABEL_ENV=mocha:cjs mocha './test/**/*.mjs'",
    "mocha:mjs": "BABEL_ENV=mocha:mjs mocha './test/**/*.mjs'",
    "prekarma": "npm run build:umd",
    "premocha:cjs": "npm run build:cjs",
    "test": "npm run mocha && npm run karma -- --single-run --browsers ChromeHeadlessNoSandbox",
    "travis": "npm run karma -- --single-run --browsers ${BROWSERS}"
  },
  "sideEffects": false
}
