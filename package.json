{
  "name": "ltsv",
  "description": "LTSV parser and formatter",
  "version": "1.1.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "browser": "./umd/ltsv.js",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "@mocha/docdash": "2.0.1",
    "babel-plugin-transform-rename-import": "2.3.0",
    "babel-preset-power-assert": "3.0.0",
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jsdoc": "3.5.5",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-rollup-preprocessor": "6.1.2",
    "karma-safari-launcher": "1.0.0",
    "lint-staged": "8.1.1",
    "mocha": "5.2.0",
    "power-assert": "1.6.1",
    "prettier": "1.16.1",
    "rollup": "0.67.3",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "cjs",
    "index.*",
    "src",
    "umd"
  ],
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.mjs",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/ltsv.js.git"
  },
  "runkitExampleFilename": "./.runkit.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "BABEL_ENV=cjs babel ./src -d ./cjs -s --ignore ./src/streams.mjs && BABEL_ENV=cjs babel ./index.mjs -o ./index.js -s",
    "build:docs": "jsdoc --configure ./.jsdocrc.js",
    "build:umd": "BABEL_ENV=umd rollup -c",
    "clean": "npx rimraf ./cjs ./umd ./index.js ./index.js.map",
    "fix": "eslint --ext .js,.mjs --fix .",
    "fixpack": "npx fixpack",
    "karma": "BABEL_ENV=karma karma start --no-browsers --file ./umd/ltsv.js",
    "lint": "eslint --ext .js,.mjs .",
    "mocha": "npm run mocha:src && npm run mocha:cjs",
    "mocha:cjs": "BABEL_ENV=mocha:cjs mocha --exclude ./test/validator.mjs './test/**/*.mjs'",
    "mocha:src": "BABEL_ENV=mocha:src mocha './test/**/*.mjs'",
    "prekarma": "npm run build:umd",
    "premocha:cjs": "npm run build:cjs",
    "test": "npm run mocha && npm run karma -- --single-run --browsers ChromeHeadlessNoSandbox",
    "travis": "npm run karma -- --single-run --browsers ${BROWSERS}"
  },
  "sideEffects": false
}
