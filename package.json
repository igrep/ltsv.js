  "description": "LTSV parser and formatter",
  "main": "./index.js",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/sasaplus1/ltsv.js.git"
  },
  "scripts": {
    "bower": "bower install",
    "clean": "make clean",
